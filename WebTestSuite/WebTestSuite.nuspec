<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2013/05/nuspec.xsd">
  <metadata>
    <id>SidSoft.WebTestSuite</id>
    <version>1.0.30.1</version>
    <authors>William Parker</authors>
    <owners>William Parker</owners>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>Set of base test to be run against websites and services</description>
    <releaseNotes>Initial release</releaseNotes>
    <copyright>SidSoft 2019</copyright>
    <tags>Test Testing Suite Web</tags>
    <repository url="https://github.com/WPNetC/WebTestSuite" />
    <dependencies>
      <group targetFramework=".NETFramework4.6">
        <dependency id="MSTest.TestFramework" version="1.3.2" exclude="Build,Analyzers" />
        <dependency id="System.Net.Http" version="4.2.0" exclude="Build,Analyzers" />
      </group>
      <group targetFramework=".NETFramework4.7">
        <dependency id="MSTest.TestFramework" version="1.3.2" exclude="Build,Analyzers" />
      </group>
    </dependencies>
    <frameworkAssemblies>
      <frameworkAssembly assemblyName="System.Net" />
      <frameworkAssembly assemblyName="System.Net.Http" targetFramework=".NETFramework4.7" />
    </frameworkAssemblies>
    <contentFiles>
      <files include="any/net46/contentFiles/StandardTests/AccessTests.cs.pp" buildAction="Content" />
      <files include="any/net47/contentFiles/StandardTests/AccessTests.cs.pp" buildAction="Content" />
      <files include="any/netstandard2.0/contentFiles/StandardTests/AccessTests.cs.pp" buildAction="Content" />
      
      <files include="any/net46/contentFiles/StandardTests/PathsAndPorts.cs.pp" buildAction="Content" />
      <files include="any/net47/contentFiles/StandardTests/PathsAndPorts.cs.pp" buildAction="Content" />
      <files include="any/netstandard2.0/contentFiles/StandardTests/PathsAndPorts.cs.pp" buildAction="Content" />
      
      <files include="any/net46/contentFiles/StandardTests/WebTestBase.cs.pp" buildAction="Content" />
      <files include="any/net47/contentFiles/StandardTests/WebTestBase.cs.pp" buildAction="Content" />
      <files include="any/netstandard2.0/contentFiles/StandardTests/WebTestBase.cs.pp" buildAction="Content" />
      
      <files include="any/net46/content/StandardTests/AccessTests.cs.pp" buildAction="Content" />
      <files include="any/net47/content/StandardTests/AccessTests.cs.pp" buildAction="Content" />
      <files include="any/netstandard2.0/content/StandardTests/AccessTests.cs.pp" buildAction="Content" />
      
      <files include="any/net46/content/StandardTests/PathsAndPorts.cs.pp" buildAction="Content" />
      <files include="any/net47/content/StandardTests/PathsAndPorts.cs.pp" buildAction="Content" />
      <files include="any/netstandard2.0/content/StandardTests/PathsAndPorts.cs.pp" buildAction="Content" />
      
      <files include="any/net46/content/StandardTests/WebTestBase.cs.pp" buildAction="Content" />
      <files include="any/net47/content/StandardTests/WebTestBase.cs.pp" buildAction="Content" />
      <files include="any/netstandard2.0/content/StandardTests/WebTestBase.cs.pp" buildAction="Content" />
    </contentFiles>
  </metadata>
</package>